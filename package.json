{
  "name": "atom-html",
  "version": "2.0.0",
  "author": "Red Pixel Themes",
  "private": true,
  "scripts": {
    "css": "postcss assets/styles/main.css -o assets/styles/main.min.css --verbose",
    "server-reload": "browser-sync start --config './browser-sync-config.js'",
    "develop": "concurrently 'npm:css -- --watch' 'npm:server-reload'",
    "format": "npx prettier ./**/*.html",
    "start": "webpack serve --mode development",
    "build": "webpack --mode production"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "browser-sync": "^2.27.7",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.17",
    "postcss-cli": "^9.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  }
}
